library(rpart)
diabetesData <- read.csv(file="pima-indians-diabetes.csv",head=TRUE,sep=",")
summary(diabetesData)
fit <- rpart(class ~ pregnant+glucose+bp+tricep+insulin+bmi+dpf+age, method="class", data=diabetesData, control=(rpart.control(minbucket=10)))
plot(fit, uniform=TRUE, main="Decision Tree 1(ii)")
text(fit, use.n=TRUE, all=TRUE, cex=.8)


